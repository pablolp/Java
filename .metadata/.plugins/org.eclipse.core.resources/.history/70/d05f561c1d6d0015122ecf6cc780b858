package figuras;

public class Circulo extends Forma {
	private int radio;
	public Circulo(int x, int y, int puntos,radio) {
		super(x, y, puntos);
		this.radio = radio;
	}

	@Override
	public double calcularArea() {
		// TODO Auto-generated method stub
		return 0;
	}

	@Override
	public String toString() {
		return "Circulo [radio=" + radio + ", getArea()=" + getArea() + ", getX()=" + getX() + ", getY()=" + getY()
				+ "]";
	}

}
